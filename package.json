{
  "name": "generator-taobao-component",
  "version": "0.0.1",
  "description": "a generator for taobao component",
  "homepage": "https://github.com/Simon-Bin/generator-tb-component#readme",
  "author": "Simon-Bin <2632003786@qq.com>",
  "files": [
    "generators"
  ],
  "main": "generators/index.js",
  "keywords": [
    "taobao",
    "yeoman-generator"
  ],
  "devDependencies": {
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "commitizen": "^4.0.4",
    "conventional-github-releaser": "^3.1.3",
    "coveralls": "^3.0.7",
    "cz-conventional-changelog": "^3.1.0",
    "eslint": "^6.6.0",
    "eslint-config-prettier": "^6.6.0",
    "eslint-config-xo": "^0.27.2",
    "eslint-plugin-prettier": "^3.1.1",
    "husky": "^3.0.9",
    "jest": "^24.8.0",
    "lint-staged": "^9.4.3",
    "prettier": "^1.19.1",
    "standard-version": "^7.1.0",
    "yeoman-assert": "^3.1.0",
    "yeoman-test": "^1.7.0"
  },
  "engines": {
    "npm": ">= 8.0.0"
  },
  "dependencies": {
    "yeoman-generator": "^2.0.1",
    "chalk": "^2.1.0",
    "yosay": "^2.0.1"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ],
    "*.json": [
      "prettier --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "eslintConfig": {
    "extends": [
      "xo",
      "prettier"
    ],
    "env": {
      "jest": true,
      "node": true
    },
    "rules": {
      "prettier/prettier": "error"
    },
    "plugins": [
      "prettier"
    ]
  },
  "scripts": {
    "pretest": "eslint .",
    "test": "jest",
    "cz": "git cz",
    "release": "standard-version",
    "github-release": "conventional-github-releaser -p angular"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Simon-Bin/generator-tb-component.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Simon-Bin/generator-tb-component/issues"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
